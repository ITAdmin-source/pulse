{
  "name": "pulse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mobile": "next dev -H 0.0.0.0",
    "dev:clean": "npm run kill-dev && npm run clean && npm run dev",
    "kill-dev": "taskkill /f /im node.exe",
    "clean": "rm -rf .next && npm cache clean --force",
    "build": "next build ",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:seed": "tsx db/seed.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:quick": "vitest run --reporter=dot --coverage=false tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:all": "npm run test:quick && npm run test:integration && npm run test:e2e"
  },
